C_CRAP=-I/Users/trent/local/include -Wall -g
LD_CRAP=-L/Users/trent/local/lib -lallegro_primitives -lallegro_image -lallegro -lallegro_main

all: earclip2

earclip2: earclip2.o dllist.o
	g++ earclip2.o dllist.o -o earclip2 $(LD_CRAP)

earclip2.o: earclip2.cpp testing.h
	g++ $(C_CRAP) -c earclip2.cpp

dllist.o: dllist.cpp dllist.h
	g++ $(C_CRAP) -c dllist.cpp

clean:
	rm -f earclip2.o dllist.o
